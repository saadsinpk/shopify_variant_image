    <script>
      var params = {};
      var options = {};
      {% for product_variant in product.variants %}
        options['{{product_variant.id}}'] = {{product_variant.options|json}};
        params['{{product_variant.id}}'] = '{{product_variant.featured_media.id}}';
      {% endfor %}
    </script>

    <script>
    $( document ).ready(function() {
      $.each( params, function( params_key, params_value) {
        $(".product__thumb-item[data-group='" + params_value + "']").hide();
      });
      var uRl = window.location.href;

      if(uRl.indexOf('variant=') > 0) {
        var get_from_url_variant_id = uRl.split("variant=")[1];
        $(".product__thumb-item[data-group='" + params[get_from_url_variant_id] + "']").show();
      } else {
        $(".product__thumb-item").each(function( index ) {
          if ($(this).css("display") != "none") {
            var p_index = $('.product-main-slide[data-index="'+$(this).index()+'"]');
            var position = $(p_index).attr("style");
            position = position.split("left: ")[1];
            out_position = position.replace(';', '');

            $('.product-main-slide[data-index="'+$(this).index()+'"]').addClass("is-selected");
            $(this).find("a").addClass("is-active");
            p_out_index = $('.product-main-slide[data-index="'+$(this).index()+'"]');
            console.log("out_position"+out_position);
            console.log('.product-main-slide[data-index="'+$(this).index()+'"]');
            $(p_out_index).closest(".flickity-slider").attr("style","left: 0px; transform: translateX(-"+out_position+");");
            return false;
          }
        });
      }
  
      $(".variant-input").click(function () {
        check_variant_select();
      });
    });
    function check_variant_select() {
      var selected_options = [];
      $( ".variant-wrapper" ).each(function( index ) {
        selected_options.push($(this).find("input[type=radio]:checked").val());
      });
      $.each( params, function( params_key, params_value) {
        $(".product__thumb-item[data-group='" + params_value + "']").hide();
      });
      $(".product-main-slide").removeClass("is-selected");
      $(".product__thumb-item a.is-active").removeClass("is-active");


      var out_position = 0;
      var p_out_index = '';

      $(".product__thumb-item").each(function( index ) {
        if ($(this).css("display") != "none") {
          var p_index = $('.product-main-slide[data-index="'+$(this).index()+'"]');
          var position = $(p_index).attr("style");
          position = position.split("left: ")[1];
          out_position = position.replace(';', '');

          $('.product-main-slide[data-index="'+$(this).index()+'"]').addClass("is-selected");
          $(this).find("a").addClass("is-active");
          p_out_index = $('.product-main-slide[data-index="'+$(this).index()+'"]');
          return false;
        }
      });
      $.each( options, function( options_key, options_value) {
        if(JSON.stringify(selected_options) == JSON.stringify(options_value)){ 
          $(".product__thumb-item[data-group='" + params[options_key] + "']").show();
          $(".product__thumb-item[data-group='" + params[options_key] + "']").find("a").addClass("is-active");

          if(params[options_key] !== '') {
            var p_index_sec = $(".product__thumb-item[data-group='" + params[options_key] + "']").index();
            var p_index_sec_style = $('.product-main-slide[data-index="'+p_index_sec+'"]').attr("style");
            p_index_sec_style = p_index_sec_style.split("left: ")[1];
            out_position = p_index_sec_style.replace(';', '');

            p_out_index = $('.product-main-slide[data-index="'+p_index_sec+'"]');
          }
          return false;
        }
      });
      $(p_out_index).closest(".flickity-slider").attr("style","left: 0px; transform: translateX(-"+out_position+");");
    }
    </script>
